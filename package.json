{
  "name": "eslint-config-fronttigger",
  "version": "0.0.6",
  "description": "fronttigger custom lint",
  "main": "index.js",
  "files": [
    "rules",
    "index.js",
    "prettierrc.js",
    "react.js",
    "stylelint.js"
  ],
  "keywords": [
    "eslint",
    "eslintconfig",
    "prettier",
    "stylelint",
    "config",
    "javascript",
    "typescript",
    "fronttigger",
    "tigger"
  ],
  "author": "fronttigger",
  "homepage": "https://github.com/fronttigger/eslint-config-fronttigger",
  "bugs": {
    "url": "https://github.com/fronttigger/eslint-config-fronttigger/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/fronttigger/eslint-config-fronttigger"
  },
  "license": "MIT",
  "scripts": {
    "lint": "eslint './**/*.{js,jsx,ts,tsx}'",
    "lint:fix": "npm run lint -- --fix",
    "lint:style": "stylelint './**/*.{js,jsx,ts,tsx}'",
    "lint:style:fix": "stylelint './**/*.{js,jsx,ts,tsx}' --fix",
    "prettier": "prettier '**/*.{json,yaml,md,js,jsx,ts,tsx}' --check",
    "prettier:fix": "prettier '**/*.{json,yaml,md,js,jsx,ts,tsx}' --write",
    "deploy": "npm publish --access=public",
    "deploy:canary": "sh ./scripts/canary-publish.sh",
    "version": "npm version"
  },
  "dependencies": {
    "@babel/eslint-parser": "^7.19.1",
    "@stylelint/postcss-css-in-js": "^0.38.0",
    "@typescript-eslint/eslint-plugin": "^5.47.0",
    "@typescript-eslint/parser": "^5.47.0",
    "eslint": "^8.30.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-prettier": "^8.5.0",
    "eslint-config-standard": "^17.0.0",
    "eslint-config-standard-jsx": "^11.0.0",
    "eslint-import-resolver-typescript": "^3.5.2",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jsx-a11y": "^6.6.1",
    "eslint-plugin-n": "^15.6.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^6.1.1",
    "eslint-plugin-react": "^7.31.11",
    "eslint-plugin-react-hooks": "^4.6.0",
    "postcss": "^8.4.21",
    "postcss-syntax": "^0.36.2",
    "prettier": "^2.8.1",
    "stylelint": "^14.16.1",
    "stylelint-config-prettier": "^9.0.4",
    "stylelint-config-standard": "^29.0.0",
    "stylelint-order": "^6.0.1"
  },
  "devDependencies": {
    "@emotion/react": "^11.10.5",
    "@emotion/styled": "^11.10.5",
    "@types/react": "^18.0.27",
    "next": "^13.1.2",
    "react": "^18.2.0",
    "typescript": "^4.9.4"
  },
  "peerDependencies": {
    "@stylelint/postcss-css-in-js": "*",
    "@typescript-eslint/eslint-plugin": "*",
    "eslint": "*",
    "eslint-plugin-import": "*",
    "eslint-plugin-jsx-a11y": "*",
    "eslint-plugin-promise": "*",
    "eslint-plugin-react": "*",
    "eslint-plugin-react-hooks": "*",
    "postcss-syntax": "*",
    "prettier": "*",
    "stylelint": "*"
  }
}
